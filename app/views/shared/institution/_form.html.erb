<%= form_with model: model, class: 'has-validation-return', url: url do |f| %>

<div class="row">
	<div class="col-md-12"><h5 class="sub-group-form">Identificação</h5></div>
</div>
<div class="row">
	<div class="col-md-6">
		<%= f.label :cnpj, "CNPJ", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<% if form_is_edit %>
			<%= f.text_field :cnpj, :class => "form-control rounded-3", disabled: true, "data-mask": "00.000.000/0000-00" %>
			<% else %>
			<%= f.text_field :cnpj, :class => "form-control rounded-3" %>
			<% end %>
			<div id="cnpj_feedback" class="invalid-feedback"></div>
		</div>
	</div>

	<div class="col-md-6">
		<%= f.label :email, "E-mail", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.email_field :email, :class => 'form-control rounded-3' %>
			<div id="email_feedback" class="invalid-feedback"></div>
		</div>
	</div>

	<div class="col-md-6">
		<%= f.label :corporate_name, "Nome", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.text_field :corporate_name, :class => 'form-control rounded-3' %>
			<div id="corporate_name_feedback" class="invalid-feedback"></div>
		</div>
	</div>

	<div class="col-md-6">
		<%= f.label :social_reason, "Razão Social", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.text_field :social_reason, :class => 'form-control rounded-3' %>
			<div id="social_reason_feedback" class="invalid-feedback"></div>
		</div>	
	</div>

	<%= f.label :qualification, "Qualificação", class: "form-label mt-1 rmb-1" %>
	<div class="input-group has-validation">
		<%= f.text_field :qualification, :class => 'form-control rounded-3' %>
		<div id="qualification_feedback" class="invalid-feedback"></div>
	</div>

	<div class="col-md-12"><h5 class="sub-group-form">Acesso</h5></div>
	<div class="col-md-6">
		<%= f.label :password, "Senha", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.password_field :password, :class => 'form-control rounded-3' %>
			<div id="password_feedback" class="invalid-feedback"></div>
		</div>
	</div>

	<div class="col-md-6">
		<%= f.label :password_confirmation, 'Confirmar senha', class: "form-label mt-1 rmb-1", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.password_field :password_confirmation, :class => 'form-control rounded-3' %>
			<div id="password_confirmation_feedback" class="invalid-feedback"></div>
		</div>
	</div>
</div>

<div class="col-md-12"><h5 class="sub-group-form">Endereço</h5></div>
<div class="row">
	<div class="col-md-6">
		<%= f.label :address, "Endereço", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.text_field :address, :class => 'form-control rounded-3' %>
			<div id="address_feedback" class="invalid-feedback"></div>
		</div>
	</div>


	<div class="col-md-6">
		<%= f.label :number, "Número", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.number_field :number, :class => 'form-control rounded-3' %>
			<div id="number_feedback" class="invalid-feedback"></div>
		</div>
	</div>

	<div class="col-md-6">
		<%= f.label :neighborhood, "Bairro", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.text_field :neighborhood, :class => 'form-control rounded-3' %>
			<div id="neighborhood_feedback" class="invalid-feedback"></div>
		</div>
	</div>


	<div class="col-md-6">
		<%= f.label :complement, "Complemento", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.text_field :complement, :class => 'form-control rounded-3' %>
			<div id="complement_feedback" class="invalid-feedback"></div>
		</div>
	</div>

	<div class="col-md-6">
		<%= f.label :zipCode, "CEP", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.number_field :zipCode, :class => 'form-control rounded-3', "data-mask": "00000-000" %>
			<div id="zipCode_feedback" class="invalid-feedback"></div>
		</div>
	</div>

</div>

<div class="row">
	<div class="col-md-6">
		<%= f.label :phone_number, "Telefone", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.text_field :phone_number, :class => 'form-control rounded-3', "data-mask": "(00) 90000-0000" %>
			<div id="phone_number_feedback" class="invalid-feedback"></div>
		</div>
	</div>

	<div class="col-md-6">
		<%= f.label :phone_number2, "Telefone 2", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.text_field :phone_number2, :class => 'form-control rounded-3', "data-mask": "(00) 90000-0000" %>
			<div id="phone_number2_feedback" class="invalid-feedback"></div>
		</div>
	</div>

</div>

<div class="col-md-12"><h5 class="sub-group-form">Pagamento</h5></div>
<div class="row">
	<div class="col-md-4">
		<%= f.label :bank_number, "Banco", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.number_field :bank_number, :class => 'form-control rounded-3' %>
			<div id="bank_number_feedback" class="invalid-feedback"></div>
		</div>
	</div>


	<div class="col-md-4">
		<%= f.label :agency_number, "Agência", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.number_field :agency_number, :class => 'form-control rounded-3', "data-mask": "99999-9" %>
			<div id="agency_number_feedback" class="invalid-feedback"></div>
		</div>
	</div>

	<div class="col-md-4">
		<%= f.label :account_number, "Conta", class: "form-label mt-1 rmb-1" %>
		<div class="input-group has-validation">
			<%= f.number_field :account_number, :class => 'form-control rounded-3' %>
			<div id="account_number_feedback" class="invalid-feedback"></div>
		</div>
	</div>

</div>

<div class="col-md-12"><h5 class="sub-group-form">Sobre</h5></div>
<div class="row">
	<div class="col-md-12">
		<div class="input-group has-validation">
			<%= f.text_field :about, :class => 'form-control rounded-3' %>
			<div id="about_feedback" class="invalid-feedback"></div>
		</div>
	</div>
</div>


<div class="row">
	<div class="col-md-6">
		<%= link_to 'Voltar', form_back_page, class: "btn btn-submit-adm mt-3" %>
	</div>
	<div class="col-md-6">
		<% if form_is_edit %>
		<%= f.submit "Atualizar", class: "btn btn-submit-adm mt-3"%>
		<% else %>
		<%= f.submit "Cadastar", class: "btn btn-submit-adm mt-3"%>
		<% end %>
	</div>
</div>

<% end %>